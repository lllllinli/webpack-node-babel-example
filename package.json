{
  "name": "webpack server example",
  "version": "1.0.0",
  "description": "use es6 in node",
  "main": "lib/index.js",
  "scripts": {
    "start": "export NODE_ENV=production && node ./dist/server.js",
    "dev": "export NODE_ENV=dev && node ./dist/server.js",
    "build-server": "webpack --progress --colors -d --config ./webpack/webpack.server.config.js",
    "build-client": "webpack --progress --colors -d --config ./webpack/webpack.client.config.js",
    "build": "concurrently \"npm run build-server\" \"npm run build-client\""
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "bootstrap": "^3.3.6",
    "css-loader": "^0.23.1",
    "file-loader": "0.8.5",
    "jquery": "^2.2.3",
    "json-loader": "^0.5.4",
    "koa": "1.2.0",
    "koa-jade": "^2.1.0",
    "koa-logger": "^1.3.0",
    "koa-proxy": "0.5.0",
    "koa-route": "^2.4.2",
    "koa-static": "^2.0.0",
    "node-fetch": "^1.5.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.3.0"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.6.5",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.6.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "isomorphic-style-loader": "^1.0.0",
    "json-loader": "0.5.4",
    "node-sass": "^3.4.2",
    "open": "0.0.5",
    "postcss-loader": "^0.8.2",
    "react-inline-css": "^2.1.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1",
    "webpack-node-externals": "^1.0.0"
  },
  "vendors": {
    "bootstrap": "=3.3.6",
    "jquery": "=2.2.3"
  }
}
